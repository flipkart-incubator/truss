// Inside placeholder
{
  //Any data can be passed to form level template like: title
  "title": "Form Title",
  //Config for form Builder
  "name": "formName[text|mandatory]",
  "class": "form[optional]",
  "type": "submit/ajax",
  "submitUrl": "http://demo3139823.mockable.io/submit-form",
  "submitWithErrors": false,
  //Allow submitting form even if error is present
  "template": "formTemplate.html[template present inside form builder | mandatory]", // Can also be  "ModuleName/templateName.html"
  // Form Template
  "dependencies": [
    {
      "path": "formComposite/classes/validators", // Any dependency which is required inside form builder, ex in case of any method passed as "Object.method"
      "name": "Validators" // Name on which methods should get populated
    }
  ],
  "pre": {
    "urlProviderMethod": "Object.method", //1 param passed is current configuration, 2nd param is call back function which should return object containing fetchConfigURL & queries
    "fetchConfigURL": "url",
    "queries": "{}[object|to be passed with fetchConfigUrl]",
    "dataProcessorMethod": "Object.method" //param1: fetchedConfig || config, param2: callback function expecting the proper config
  },
  "globals": {
  },
  "sets": [{
    "name": "SetName",
    "template": "Set Template",
    "fields": {
      "eventListeners": ["keyup", "hover"],
      "name": "",
      "label": "",
      "value": ""
    }
  }]
}